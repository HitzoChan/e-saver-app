
{
  "version": 2,
  "builds": [
    {
      "src": "lib/main.dart",
      "use": "@vercel/flutter"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/lib/main.dart"
    }
  ],
  "crons": [
    {
      "path": "/api/scheduled-notifications?type=morning",
      "schedule": "0 8 * * *"
    },
    {
      "path": "/api/scheduled-notifications?type=afternoon",
      "schedule": "0 14 * * *"
    },
    {
      "path": "/api/scheduled-notifications?type=evening",
      "schedule": "0 20 * * *"
    },
    {
      "path": "/api/scheduled-notifications?type=weekly",
      "schedule": "0 9 * * 1"
    }
  ]
}
